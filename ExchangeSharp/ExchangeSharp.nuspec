<?xml version="1.0"?>
<package>
  <metadata>
    <id>DigitalRuby.ExchangeSharp</id>
    <version>0.1.8.9</version>
    <title>Exchange Sharp - C# API for cryptocurrency, stock and other exchanges</title>
    <authors>jjxtra</authors>
    <owners>jjxtra</owners>
    <licenseUrl>https://github.com/jjxtra/ExchangeSharp/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/jjxtra/ExchangeSharp</projectUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>ExchangeSharp is a C# API for working with various exchanges for stocks and cryptocurrency. Binance, Bitfinex, Bithumb, Bitstamp, Bittrex, Gemini, GDAX, Kraken and Poloniex are supported.</description>
    <releaseNotes>.NET standard and .NET 4.7 dual targeting</releaseNotes>
    <copyright>Copyright 2017, Digital Ruby, LLC - www.digitalruby.com</copyright>
    <tags>C# API bitcoin exchange cryptocurrency stock trade trader coin litecoin ethereum gdax cash poloniex gemini bitfinex kraken bittrex binance iota cardano eos cordana ripple xrp eth btc</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.7">
        <dependency id="System.Security.Cryptography.ProtectedData" version="4.4.0" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="10.0.3" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="System.Security.Cryptography.ProtectedData" version="4.4.0" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="10.0.3" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Web" targetFramework=".NETFramework4.7" />
      <frameworkAssembly assemblyName="System.Windows.Forms.DataVisualization" targetFramework=".NETFramework4.7" />
      <frameworkAssembly assemblyName="System.Windows.Forms" targetFramework=".NETFramework4.7" />
    </frameworkAssemblies>
  </metadata>
</package>